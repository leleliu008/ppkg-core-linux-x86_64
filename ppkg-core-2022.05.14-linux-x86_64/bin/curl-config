prefix=/home/leleliu008/.ppkg/install.d/curl
exec_prefix=${prefix}
includedir=${prefix}/include
cppflag_curl_staticlib=-DCURL_STATICLIB

usage()
{
    cat <<EOF
Usage: curl-config [OPTION]

Available values for OPTION include:

  --built-shared says 'yes' if libcurl was built shared
  --ca        ca bundle install path
  --cc        compiler
  --cflags    pre-processor and compiler flags
  --checkfor [version] check for (lib)curl of the specified version
  --configure the arguments given to configure when building curl
  --features  newline separated list of enabled features
  --help      display this help and exit
  --libs      library linking information
  --prefix    curl install prefix
  --protocols newline separated list of enabled protocols
  --ssl-backends output the SSL backends libcurl was built to support
  --static-libs static libcurl library linking information
  --version   output version information
  --vernum    output the version information as a number (hexadecimal)
EOF

    exit $1
}

if test $# -eq 0; then
    usage 1
fi

while test $# -gt 0; do
    case "$1" in
    # this deals with options in the style
    # --option=value and extracts the value part
    # [not currently used]
    -*=*) value=`echo "$1" | sed 's/[-_a-zA-Z0-9]*=//'` ;;
    *) value= ;;
    esac

    case "$1" in
    --built-shared)
        echo no
        ;;

    --ca)
        echo "/etc/ssl/certs/ca-certificates.crt"
        ;;

    --cc)
        echo "/home/leleliu008/Downloads/musl-1.2.3/musl-libc/bin/musl-gcc"
        ;;

    --prefix)
        echo "$prefix"
        ;;

    --feature|--features)
        for feature in AsynchDNS HSTS HTTPS-proxy IDN IPv6 Largefile NTLM NTLM_WB SSL TLS-SRP UnixSockets alt-svc ""; do
            test -n "$feature" && echo "$feature"
        done
        ;;

    --protocols)
        for protocol in DICT FILE FTP FTPS GOPHER GOPHERS HTTP HTTPS IMAP IMAPS MQTT POP3 POP3S RTSP SMB SMBS SMTP SMTPS TELNET TFTP; do
            echo "$protocol"
        done
        ;;

    --version)
        echo libcurl 7.83.0
        exit 0
        ;;

    --checkfor)
        checkfor=$2
        cmajor=`echo $checkfor | cut -d. -f1`
        cminor=`echo $checkfor | cut -d. -f2`
        # when extracting the patch part we strip off everything after a
        # dash as that's used for things like version 1.2.3-CVS
        cpatch=`echo $checkfor | cut -d. -f3 | cut -d- -f1`

        vmajor=`echo 7.83.0 | cut -d. -f1`
        vminor=`echo 7.83.0 | cut -d. -f2`
        # when extracting the patch part we strip off everything after a
        # dash as that's used for things like version 1.2.3-CVS
        vpatch=`echo 7.83.0 | cut -d. -f3 | cut -d- -f1`

        if test "$vmajor" -gt "$cmajor"; then
            exit 0;
        fi
        if test "$vmajor" -eq "$cmajor"; then
            if test "$vminor" -gt "$cminor"; then
                exit 0
            fi
            if test "$vminor" -eq "$cminor"; then
                if test "$cpatch" -le "$vpatch"; then
                    exit 0
                fi
            fi
        fi

        echo "requested version $checkfor is newer than existing 7.83.0"
        exit 1
        ;;

    --vernum)
        echo 075300
        exit 0
        ;;

    --help)
        usage 0
        ;;

    --cflags)
        if test "X$cppflag_curl_staticlib" = "X-DCURL_STATICLIB"; then
          CPPFLAG_CURL_STATICLIB="-DCURL_STATICLIB "
        else
          CPPFLAG_CURL_STATICLIB=""
        fi
        if test "X${prefix}/include" = "X/usr/include"; then
          echo "$CPPFLAG_CURL_STATICLIB"
        else
          echo "${CPPFLAG_CURL_STATICLIB}-I${prefix}/include"
        fi
        ;;

    --libs)
        if test "X${exec_prefix}/lib" != "X/usr/lib" -a "X${exec_prefix}/lib" != "X/usr/lib64"; then
           CURLLIBDIR="-L${exec_prefix}/lib "
        else
           CURLLIBDIR=""
        fi
        if test "Xno" = "Xno"; then
          echo ${CURLLIBDIR}-lcurl -lidn2 -lssl -lcrypto -lssl -lcrypto
        else
          echo ${CURLLIBDIR}-lcurl
        fi
        ;;
    --ssl-backends)
        echo "OpenSSL"
        ;;

    --static-libs)
        if test "Xyes" != "Xno" ; then
          echo ${exec_prefix}/lib/libcurl.a -L/home/leleliu008/.ppkg/installing.d/curl -L/home/leleliu008/.ppkg/install.d/pkg-config/lib -L/home/leleliu008/.ppkg/install.d/openssl/lib -L/home/leleliu008/.ppkg/install.d/zlib/lib -L/home/leleliu008/.ppkg/install.d/brotli/lib -L/home/leleliu008/.ppkg/install.d/libidn2/lib -L/home/leleliu008/Downloads/musl-1.2.3/musl-libc/lib --static -static -L/home/leleliu008/.ppkg/install.d/openssl/lib -L/home/leleliu008/.ppkg/install.d/libidn2/lib -lidn2 -lssl -lcrypto -lssl -lcrypto
        else
          echo "curl was built with static libraries disabled" >&2
          exit 1
        fi
        ;;

    --configure)
        echo " '--prefix=/home/leleliu008/.ppkg/install.d/curl' '--disable-option-checking' '--disable-rpath' '--disable-nls' '--enable-largefile' 'CC=/home/leleliu008/Downloads/musl-1.2.3/musl-libc/bin/musl-gcc' 'CFLAGS= -static' 'CXX=/usr/bin/c++' 'CXXFLAGS= -static' 'CPP=/usr/bin/cpp' 'CPPFLAGS=-I/home/leleliu008/.ppkg/install.d/pkg-config/include -I/home/leleliu008/.ppkg/install.d/openssl/include -I/home/leleliu008/.ppkg/install.d/zlib/include -I/home/leleliu008/.ppkg/install.d/brotli/include -I/home/leleliu008/.ppkg/install.d/libidn2/include -I/home/leleliu008/Downloads/linux-5.17/dest/include' 'LDFLAGS=-L/home/leleliu008/.ppkg/installing.d/curl -L/home/leleliu008/.ppkg/install.d/pkg-config/lib -L/home/leleliu008/.ppkg/install.d/openssl/lib -L/home/leleliu008/.ppkg/install.d/zlib/lib -L/home/leleliu008/.ppkg/install.d/brotli/lib -L/home/leleliu008/.ppkg/install.d/libidn2/lib -L/home/leleliu008/Downloads/musl-1.2.3/musl-libc/lib --static -static' 'AR=/usr/bin/ar' 'RANLIB=/usr/bin/ranlib' 'PKG_CONFIG=/home/linuxbrew/.linuxbrew/bin/pkg-config' 'PKG_CONFIG_PATH=/home/linuxbrew/.linuxbrew/opt/python@3.10/lib/pkgconfig:/home/leleliu008/.ppkg/install.d/libidn2/lib/pkgconfig:/home/leleliu008/.ppkg/install.d/brotli/lib/pkgconfig:/home/leleliu008/.ppkg/install.d/zlib/lib/pkgconfig:/home/leleliu008/.ppkg/install.d/openssl/lib/pkgconfig:/home/leleliu008/.ppkg/install.d/pkg-config/lib/pkgconfig' 'PKG_CONFIG_LIBDIR=' '--disable-debug' '--enable-static' '--disable-shared' '--with-ssl=/home/leleliu008/.ppkg/install.d/openssl' '--enable-optimize' '--enable-pthreads'"
        ;;

    *)
        echo "unknown option: $1"
        usage 1
        ;;
    esac
    shift
done

exit 0
